{
  "name": "epam-cdisc-electron",
  "version": "0.6.0",
  "private": true,
  "main": "./build/main/index.js",
  "description": "epam-cdisc-electron",
  "homepage": "./",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "del /Q build",
    "copy:main": "copyfiles -f ./assets/main/* ./build/main",
    "build:main": "tsc -p ./src/main && npm run copy:main",
    "build:renderer": "npx webpack --mode production",
    "build": "npm run build:renderer && npm run build:main",
    "start": "npx electron .",
    "predev": "del /Q build",
    "dev:main": "tsc -w -p ./src/main && npm run copy:main",
    "dev:renderer": "npx env-cmd -f .env.development webpack serve --mode development",
    "dev:electron": "wait-on tcp:5000 && wait-on ./build/main/index.js && npx env-cmd -f .env.development npx electronmon .",
    "dev": "concurrently -k \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:electron\"",
    "package:icons": "del /Q app-assets && npx electron-icon-builder -f --input=./assets/main/app-icon.png --output=./app-assets",
    "package": "npm run package:icons && electron-forge package",
    "make": "npm run package:icons && npx env-cmd -f .env.codesign electron-forge make",
    "lint:code": "eslint . --ext js,ts,tsx --fix",
    "lint:types": "tsc --noEmit",
    "lint:styles": "stylelint \"./src/**/*.{ts,tsx}\" --fix",
    "lint": "npm run lint:code && npm run lint:types && npm run lint:styles",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "EPAM Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-dmg": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "del-cli": "^5.0.0",
    "electron": "^22.3.25",
    "electron-icon-builder": "^2.0.1",
    "electronmon": "^2.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.8.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@mui/styled-engine-sc": "^5.10.6",
    "@reduxjs/toolkit": "^1.9.0",
    "dayjs": "^1.11.6",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "fast-xml-parser": "^4.0.12",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.6"
  }
}
